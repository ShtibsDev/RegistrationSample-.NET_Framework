<Application x:Class="RegistrationSample.DesktopUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:RegistrationSample.DesktopUI.ViewModels"
             xmlns:views="clr-namespace:RegistrationSample.DesktopUI.Views">
             
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:ShellViewModel}">
            <views:ShellView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:LoginViewModel}">
            <views:LoginView/>
        </DataTemplate> 
    </Application.Resources>
</Application>
